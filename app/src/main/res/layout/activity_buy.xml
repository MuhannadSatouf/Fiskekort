<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".buy"
    android:background="@drawable/fisk"
    >

    <Button
        android:id="@+id/oneDayButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buy 1 Day"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="79dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/oneMonthButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buy 1 Month"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="262dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/sixMonthButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buy 6 Months"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="395dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/yearButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buy 1 year"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="559dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>